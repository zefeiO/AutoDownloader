{
    "name": "AudioDownloader",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js", "inject.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Audio Downloader"
    },
    "web_accessible_resources" : [
        "extract.js"
    ]
}